    "lib": ["DOM", "ES6"],
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
}